{
    "version": 2,
    "builds": [
      {
        "src": "/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/allStatus",
        "methods": ["GET"],
        "headers": ["Origin", "Accept", "Content-Type"]
      },
      {
        "src": "/setStatus",
        "methods": ["POST"],
        "headers": ["Origin", "Accept", "Content-Type"]
      },
      {
        "src": "/lastStatus",
        "methods": ["GET"],
        "headers": ["Origin", "Accept", "Content-Type"]
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "OPTIONS, GET, POST, PUT, DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ]
  }
  